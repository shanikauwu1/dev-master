					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Alumni Sans\"",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/alumni-sans/alumni-sans-500-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Alumni Sans\", sans-serif",
					"name": "Alumni Sans",
					"slug": "alumni-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
